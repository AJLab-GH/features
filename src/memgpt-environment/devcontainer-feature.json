{
  "customizations": {
    "features": {
      "ghcr.io/itsmechlark/features/postgresql": {},
      "ghcr.io/robinmordasiewicz/features/preinstall-extensions": {
        "extensions": [
          "ms-ossdata.vscode-postgresql",
          "mtxr.sqltools",
          "mtxr.sqltools-driver-pg"
        ]
      },
      "ghcr.io/rocker-org/devcontainer-features/miniforge": {}
    },
    "vscode": {
      "settings": {
        "sqltools.connections": [
          {
            "database": "postgres",
            "driver": "PostgreSQL",
            "name": "Container database",
            "password": "postgres",
            "port": 5432,
            "previewLimit": 50,
            "server": "localhost",
            "username": "postgres"
          }
        ]
      }
    }
  },
  "description": "Create the python environment for memgpt",
  "id": "memgpt-environment",
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils",
    "ghcr.io/robinmordasiewicz/features/update-conda"
  ],
  "name": "MeMGPT Python Environment",
  "version": "0.0.8"
}
